@model IEnumerable<SAPPub.Core.Entities.Establishment>




<govuk-summary-list>
    @foreach(var modelItem in Model)
    {
        <govuk-summary-list-row>
            <govuk-summary-list-row-key>
                Establishment Name
            </govuk-summary-list-row-key>
            <govuk-summary-list-row-value>
                @modelItem.EstablishmentName
            </govuk-summary-list-row-value>
            <govuk-summary-list-row-actions>
                <govuk-summary-list-row-action href="@Url.Action("Data", "Data", new {id =modelItem.URN})"  visually-hidden-text="name">View</govuk-summary-list-row-action>
            </govuk-summary-list-row-actions>
        </govuk-summary-list-row>
    }
</govuk-summary-list>